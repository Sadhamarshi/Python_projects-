import random
import string
no_alpha=string.ascii_letters
no_digit=string.digits
no_symbols=string.punctuation
password=''
a=int(input('no of alphabets: '))
b=int(input('no of digits: '))
c=int(input('no of symbols: '))
for i in range(1,a+1):
    letter=random.choice(no_alpha)
    password+=letter
for i in range(1,b+1):
    digit=random.choice(no_digit)
    password+=digit
for o in range(1,c+1):
    symbol=random.choice(no_symbols)
    password+=symbol
print(password)
